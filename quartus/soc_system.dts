		};

		i2c@0xffc04000 {
			compatible = "snps,designware-i2c-24.1", "snps,designware-i2c";
			reg = <0xffc04000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0x9e 0x04>;
			emptyfifo_hold_master = <0x01>;
			status = "okay";
		};

		i2c@0xffc05000 {
			compatible = "snps,designware-i2c-24.1", "snps,designware-i2c";
			reg = <0xffc05000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0x9f 0x04>;
			emptyfifo_hold_master = <0x01>;
			status = "okay";
		};

		i2c@0xffc06000 {
			compatible = "snps,designware-i2c-24.1", "snps,designware-i2c";
			reg = <0xffc06000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0xa0 0x04>;
