			#address-cells = <0x01>;
			#size-cells = <0x00>;
			bus-num = <0x00>;
			num-chipselect = <0x04>;
			status = "okay";
		};

		flash@0xff705000 {
			compatible = "cadence,qspi-24.1", "cadence,qspi", "cdns,qspi-nor";
			reg = <0xff705000 0x100 0xffa00000 0x100>;
