/dts-v1/;

/ {
	model = "ALTR,soc_system";
	compatible = "ALTR,soc_system";
	#address-cells = <0x01>;
	#size-cells = <0x01>;

	cpus {
		#address-cells = <0x01>;
		#size-cells = <0x00>;

		cpu@0x0 {
			device_type = "cpu";
			compatible = "arm,cortex-a9-24.1", "arm,cortex-a9";
			reg = <0x00>;
		};

		cpu@0x1 {
			device_type = "cpu";
			compatible = "arm,cortex-a9-24.1", "arm,cortex-a9";
			reg = <0x01>;
		};
	};

	memory {
		device_type = "memory";
		reg = <0xffff0000 0x10000 0x00 0x80000000>;
	};

	sopc@0 {
		device_type = "soc";
		ranges;
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		compatible = "ALTR,avalon", "simple-bus";
		bus-frequency = <0x00>;

		bridge@0xff200000 {
			compatible = "altr,bridge-24.1", "simple-bus";
			reg = <0xff200000 0x200000>;
			#address-cells = <0x02>;
			#size-cells = <0x01>;
			ranges = <0x01 0x20000 0xff220000 0x10 0x01 0x00 0xff200000 0x1000>;

			unknown@0x100020000 {
				compatible = "unknown,unknown-18.0";
				reg = <0x01 0x20000 0x10>;
			};

			unknown@0x100000000 {
				compatible = "unknown,unknown-1.0";
				reg = <0x01 0x00 0x1000>;
				interrupt-parent = <0x01>;
				interrupts = <0x00 0x28 0x04>;
			};
		};

		intc@0xfffed000 {
			compatible = "arm,cortex-a9-gic-24.1", "arm,cortex-a9-gic";
			reg = <0xfffed000 0x1000 0xfffec100 0x100>;
			reg-names = "axi_slave0", "axi_slave1";
			interrupt-controller;
			#interrupt-cells = <0x03>;
			linux,phandle = <0x01>;
		};

		L2-cache@0xfffef000 {
			compatible = "arm,pl310-cache-24.1", "arm,pl310-cache";
			reg = <0xfffef000 0x1000>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0x26 0x04>;
			cache-level = <0x02>;
		};

		dma@0xffe01000 {
			compatible = "arm,pl330-24.1", "arm,pl330", "arm,primecell";
			reg = <0xffe01000 0x1000>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0x68 0x04>;
			#dma-cells = <0x01>;
			#dma-channels = <0x08>;
			#dma-requests = <0x20>;
			clock-names = "apb_pclk";
			copy-align = <0x03>;
			nr-irqs = <0x09>;
			nr-valid-peri = <0x09>;
		};

		sysmgr@0xffd08000 {
			compatible = "altr,sys-mgr-24.1", "altr,sys-mgr", "syscon";
			reg = <0xffd08000 0x400>;
			cpu1-start-addr = <0xffd080c4>;
		};

		clkmgr@0xffd04000 {
			compatible = "altr,clk-mgr-24.1", "altr,clk-mgr";
			reg = <0xffd04000 0x1000>;

			clock_tree {
				#size-cells = <0x00>;
				#address-cells = <0x01>;

				sdram_pll {
					compatible = "altr,socfpga-pll-clock";
					reg = <0xc0>;
					#clock-cells = <0x00>;
					#address-cells = <0x01>;
					#size-cells = <0x00>;

					ddr_dqs_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0xc8>;
						#clock-cells = <0x00>;
					};

					ddr_2x_dqs_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0xcc>;
						#clock-cells = <0x00>;
					};

					ddr_dq_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0xd0>;
						#clock-cells = <0x00>;
					};

					s2f_usr2_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0xd4>;
						#clock-cells = <0x00>;
					};
				};

				periph_pll {
					compatible = "altr,socfpga-pll-clock";
					reg = <0x80>;
					#clock-cells = <0x00>;
					#address-cells = <0x01>;
					#size-cells = <0x00>;

					per_nand_mmc_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0x94>;
						#clock-cells = <0x00>;
					};

					per_base_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0x98>;
						#clock-cells = <0x00>;
					};

					per_qspi_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0x90>;
						#clock-cells = <0x00>;
					};

					s2f_usr1_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0x9c>;
						#clock-cells = <0x00>;
					};

					emac0_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0x88>;
						#clock-cells = <0x00>;
					};

					emac1_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0x8c>;
						#clock-cells = <0x00>;
					};
				};

				main_pll {
					compatible = "altr,socfpga-pll-clock";
					reg = <0x40>;
					#clock-cells = <0x00>;
					#address-cells = <0x01>;
					#size-cells = <0x00>;

					cfg_s2f_usr0_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0x5c>;
						#clock-cells = <0x00>;
					};

					main_qspi_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0x54>;
						#clock-cells = <0x00>;
					};

					dbg_base_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0x50>;
						#clock-cells = <0x00>;
						div-reg = <0xe8 0x00 0x09>;
					};

					mpuclk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0x48>;
						#clock-cells = <0x00>;
						div-reg = <0xe0 0x00 0x09>;
					};

					mainclk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0x4c>;
						#clock-cells = <0x00>;
						div-reg = <0xe4 0x00 0x09>;
					};

					main_nand_sdmmc_clk {
						compatible = "altr,socfpga-perip-clk";
						reg = <0x58>;
						#clock-cells = <0x00>;
					};
				};

				mpu_l2_ram_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					fixed-divider = <0x02>;
				};

				l4_main_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0x60 0x00>;
				};

				l3_mp_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0x60 0x01>;
					div-reg = <0x64 0x00 0x02>;
				};

				l3_sp_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					div-reg = <0x64 0x02 0x02>;
				};

				l4_mp_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0x60 0x02>;
					div-reg = <0x64 0x04 0x03>;
				};

				l4_sp_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0x60 0x03>;
					div-reg = <0x64 0x07 0x03>;
				};

				dbg_at_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0x60 0x04>;
					div-reg = <0x68 0x00 0x02>;
				};

				dbg_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0x60 0x05>;
					div-reg = <0x68 0x02 0x02>;
				};

				dbg_trace_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0x60 0x06>;
					div-reg = <0x6c 0x00 0x03>;
				};

				dbg_timer_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0x60 0x07>;
				};

				cfg_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0x60 0x08>;
				};

				h2f_user0_clock {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0x60 0x09>;
				};

				emac_0_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xa0 0x00>;
				};

				emac_1_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xa0 0x01>;
				};

				usb_mp_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xa0 0x02>;
					div-reg = <0xa4 0x00 0x03>;
				};

				spi_m_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xa0 0x03>;
					div-reg = <0xa4 0x03 0x03>;
				};

				can0_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xa0 0x04>;
					div-reg = <0xa4 0x06 0x03>;
				};

				can1_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xa0 0x05>;
					div-reg = <0xa4 0x09 0x03>;
				};

				gpio_db_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xa0 0x06>;
					div-reg = <0xa8 0x00 0x18>;
				};

				h2f_user1_clock {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xa0 0x07>;
				};

				sdmmc_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xa0 0x08>;
				};

				nand_x_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xa0 0x09>;
				};

				nand_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xa0 0x0a>;
					fixed-divider = <0x04>;
				};

				qspi_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xa0 0x0b>;
				};

				ddr_dqs_clk_gate {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xd8 0x00>;
				};

				ddr_2x_dqs_clk_gate {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xd8 0x01>;
				};

				ddr_dq_clk_gate {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xd8 0x02>;
				};

				h2f_user2_clock {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
					clk-gate = <0xd8 0x03>;
				};

				l3_main_clk {
					compatible = "altr,socfpga-gate-clk";
					#clock-cells = <0x00>;
				};

				mpu_periph_clk {
					compatible = "altr,socfpga-perip-clk";
					#clock-cells = <0x00>;
					reg = <0x00>;
					fixed-divider = <0x04>;
				};
			};
		};

		rstmgr@0xffd05000 {
			compatible = "altr,rst-mgr-24.1", "altr,rst-mgr", "syscon";
			reg = <0xffd05000 0x100>;
			#reset-cells = <0x01>;
			altr,modrst-offset = <0x10>;
		};

		fpgamgr@0xff706000 {
			compatible = "altr,fpga-mgr-24.1", "altr,fpga-mgr", "altr,socfpga-fpga-mgr";
			reg = <0xff706000 0x1000 0xffb90000 0x100>;
			reg-names = "axi_slave0", "axi_slave1";
			interrupt-parent = <0x01>;
			interrupts = <0x00 0xaf 0x04>;
			transport = "mmio";
		};

		serial@0xffc02000 {
			compatible = "snps,dw-apb-uart-24.1", "snps,dw-apb-uart";
			reg = <0xffc02000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0xa2 0x04>;
			reg-io-width = <0x04>;
			reg-shift = <0x02>;
			status = "okay";
		};

		serial@0xffc03000 {
			compatible = "snps,dw-apb-uart-24.1", "snps,dw-apb-uart";
			reg = <0xffc03000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0xa3 0x04>;
			reg-io-width = <0x04>;
			reg-shift = <0x02>;
			status = "disabled";
		};

		timer@0xffc08000 {
			compatible = "snps,dw-apb-timer-sp-24.1", "snps,dw-apb-timer-sp";
			reg = <0xffc08000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0xa7 0x04>;
			clock-names = "timer";
		};

		timer@0xffc09000 {
			compatible = "snps,dw-apb-timer-sp-24.1", "snps,dw-apb-timer-sp";
			reg = <0xffc09000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0xa8 0x04>;
			clock-names = "timer";
		};

		timer@0xffd00000 {
			compatible = "snps,dw-apb-timer-osc-24.1", "snps,dw-apb-timer-osc";
			reg = <0xffd00000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0xa9 0x04>;
			clock-names = "timer";
		};

		timer@0xffd01000 {
			compatible = "snps,dw-apb-timer-osc-24.1", "snps,dw-apb-timer-osc";
			reg = <0xffd01000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0xaa 0x04>;
			clock-names = "timer";
		};

		timer@0xffd02000 {
			compatible = "snps,dw-wdt-24.1", "snps,dw-wdt";
			reg = <0xffd02000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0xab 0x04>;
			clock-names = "timer";
		};

		timer@0xffd03000 {
			compatible = "snps,dw-wdt-24.1", "snps,dw-wdt";
			reg = <0xffd03000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0xac 0x04>;
			clock-names = "timer";
		};

		gpio@0xff708000 {
			compatible = "snps,dw-apb-gpio", "snps,dw-gpio-24.1", "snps,dw-gpio";
			reg = <0xff708000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0xa4 0x04>;
			#gpio-cells = <0x02>;
			gpio-controller;
			#address-cells = <0x01>;
			#size-cells = <0x00>;

			gpio-controller@0 {
				compatible = "snps,dw-apb-gpio-port";
				gpio-controller;
				#gpio-cells = <0x02>;
				snps,nr-gpios = <0x1d>;
				reg = <0x00>;
				interrupt-controller;
				#interrupt-cells = <0x02>;
				interrupts = <0x00 0xa4 0x04>;
				interrupt-parent = <0x01>;
			};
		};

		gpio@0xff709000 {
			compatible = "snps,dw-apb-gpio", "snps,dw-gpio-24.1", "snps,dw-gpio";
			reg = <0xff709000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0xa5 0x04>;
			#gpio-cells = <0x02>;
			gpio-controller;
			#address-cells = <0x01>;
			#size-cells = <0x00>;

			gpio-controller@0 {
				compatible = "snps,dw-apb-gpio-port";
				gpio-controller;
				#gpio-cells = <0x02>;
				snps,nr-gpios = <0x1d>;
				reg = <0x00>;
				interrupt-controller;
				#interrupt-cells = <0x02>;
				interrupts = <0x00 0xa5 0x04>;
				interrupt-parent = <0x01>;
			};
		};

		gpio@0xff70a000 {
			compatible = "snps,dw-apb-gpio", "snps,dw-gpio-24.1", "snps,dw-gpio";
			reg = <0xff70a000 0x100>;
			interrupt-parent = <0x01>;
			interrupts = <0x00 0xa6 0x04>;
			#gpio-cells = <0x02>;
			gpio-controller;
			#address-cells = <0x01>;
			#size-cells = <0x00>;

			gpio-controller@0 {
				compatible = "snps,dw-apb-gpio-port";
				gpio-controller;
				#gpio-cells = <0x02>;
				snps,nr-gpios = <0x1b>;
				reg = <0x00>;
				interrupt-controller;
				#interrupt-cells = <0x02>;
				interrupts = <0x00 0xa6 0x04>;
				interrupt-parent = <0x01>;
